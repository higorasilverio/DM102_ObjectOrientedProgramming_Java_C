package br.inatel.dm102;

import java.util.Date;

public class Camarote extends Ingresso {

	private String tamanhoCamiseta;

	public Camarote(long numero, float valorIngresso, Date dataHoraCompra, String tamanhoCamiseta) {
		super(numero, valorIngresso, dataHoraCompra);
		this.tamanhoCamiseta = tamanhoCamiseta;
	}

	public void setTamanhoCamiseta(String tamanhoCamiseta) {
		this.tamanhoCamiseta = tamanhoCamiseta;
	}

}
