package br.inatel.dm102;

public class Corpo {

	private String tipo;
	private String cor;

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public void setCor(String cor) {
		this.cor = cor;
	}

	public String getTipo() {
		return tipo;
	}

	public String getCor() {
		return cor;
	}

}
